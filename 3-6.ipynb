      "source": [
        "6.印星星(三)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "n = 5\n",
        "midpoint = n // 2\n",
        "\n",
        "for i in range(n):\n",
        "    if i <= midpoint:\n",
        "        stars = 2 * i + 1\n",
        "        spaces = midpoint - i\n",
        "    else:\n",
        "        stars = 2 * (n - i - 1) + 1\n",
        "        spaces = i - midpoint\n",
        "\n",
        "    print(\" \" * spaces, end=\"\")\n",
        "\n",
        "    print(\"*\" * stars)\n"
      ],
